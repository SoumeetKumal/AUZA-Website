{
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|woff|woff2|ttf|eot|css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.@(html|json|xml)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
